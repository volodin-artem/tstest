{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": [
    "plugin:react/recommended",
    "airbnb"
  ],
  "overrides": [
  ],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": [
    "react"
  ],
  "rules": {
    "linebreak-style": "off",
    "quotes": "off",
    "prefer-template": "off",
    "import/extensions" : "off",
    "no-useless-concat": "off",
    "react/prop-types": "off",
    "react/destructuring-assignment": "off"
  }
}
